{
  "name": "@egamagz/paska-ovo",
  "version": "1.0.8",
  "exports": "./mod.ts",
  "lock": false,
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  },
  "imports": {
    "@/": "./src/",
    "@std/testing": "https://deno.land/std@0.223.0/assert/mod.ts"
  },
  "tasks": {
    "doc": "deno doc --html --name='Paska Ovo' src",
    "www": "deno task --cwd=www dev",
    "build-www": "deno task --cwd=www build"
  },
  "publish": {
    "exclude": [
      "www",
      "test",
      ".github"
    ]
  },
  "exclude": [
    "www/"
  ],
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": [
        "eqeqeq"
      ]
    }
  }
}
